# provision for alternative tuning
def adjustTuning (newtuning): # eg ["E","B","E","G#","B","E"]
    global currentTuning, semitones
    currentTuning = newtuning # keep record of change
    noteindex = 0
    for note in defaultTuning:
        newnote = newtuning[noteindex]
        tuning = tunings[note] # possible variations to the default tuning of this string
        semitones[noteindex] = tuning[newnote] # construct semitone addition/subtraction list
        noteindex += 1
    
# create a new tuning from a given string of characters
def adjustTuningFromStr (newtuningstr):
    tuninglist = []
    indx = 0
    for notechar in newtuningstr:
        if notechar == '#' or notechar == 'b':
            tuninglist[indx-1] += notechar
        else:
            if "ABCDEFG".find(notechar) == -1:
                print("Tuning must be a valid note name - \'" + notechar + "\' is not a vaild note name.")
                exit()
            else:
                tuninglist.append (notechar)
                indx += 1
    adjustTuning(tuninglist)
        
# return the notename played for a given startnote name and fret
def getNote (startnote, fret):
    keyindex = 0
    fretnote = fret
    for key in keys:
        if key == startnote:
            fretnote += keyindex
            break
        else:
            keyindex += 1
    if fretnote &gt;= 12:
        fretnote %= 12
    return keys[fretnote]

def setHand(newhand): # set player's handedness, "R" or "L". "R" is set by default
    global handed
    handed = newhand

def currTuning(): # return current tuning as a string of characters, rather than a list
    s = ""
    return s.join(currentTuning)

def currDerivation(derivation): # return a string indicating the current derivation, example: C[A]GED
    s = ""
    for cage in ["C", "A", "G", "E", "D"]:
        if cage == derivation:
            s+= '['
            s+= derivation
            s+= ']'
        else:
            s+= cage
    return s
    
# construct chord variations based on the CAGED system.    
def doCagedChord (conn, handle, path, webpage, key, dr, chordname, comment, icount, xml, web, sql, wantnotelist, scaleopt):
    intervals = caged[key]
    index = 0
    
    for cage in ["C", "A", "G", "E", "D"]:
        nl = [] # blank notelist
        clone = chords[cage][cage + chordname][0]
        if xml:
            handle.write("&lt;chord instrument='guitar' strings='6' tuning='" + currTuning() + "' position='" + str(clone[0]) + "'&gt;\r\n") # write chord metadata
            handle.write("&lt;keynote&gt;" + key + "&lt;/keynote&gt;\r\n")
        filename = key + chordname + "_" + str(index + 2) + ".svg"
        
        if xml:
            handle.write("&lt;name&gt;" + chordname + "&lt;/name&gt;\r\n")
            # now output the fret positions
            handle.write("&lt;fretpos&gt;")
        chordargs = [] # prepare to construct args for SVG generator
        lowestfret = 25 # starting point for lowest fret detection
        dbfrets = ""
        for fretpos in range(1,7):
            # ignore fingering, use only 1st item in each sublist
            if  clone[fretpos][0] == -1: 
                if xml:
                    handle.write("X") # write X to XML file
                chordargs.append ("X") # prepare fret positions for SVG generator
                nl.append(" ")
            else:
                calcfret = clone[fretpos][0] + intervals[index] + semitones[fretpos-1]
                chordargs.append(calcfret) # prepare fret positions for SVG generator
                if xml:
                    handle.write(str(calcfret)) # write the fret posn to XML file
                dbfrets += str(calcfret)
                nl.append(getNote(currentTuning[fretpos-1],calcfret))
                if  calcfret &lt; lowestfret:
                    lowestfret = calcfret
                    
            if fretpos &lt; 6: # only put spaces betwixt fret posns.
                if xml:
                    handle.write(' ')
                dbfrets += ' '

        index += 1
        # now prepare arguments for SVG chord generator
        chordargs.append (0) # no capo
        cut = 0 # assume no cut
        if lowestfret &gt; 2:
            cut = lowestfret - 2
        chordargs.append (cut) # apply any cut
        chordargs.append (handed) # set player's handedness 
        chordargs.append(key + chordname)
        chordargs.append(comment + ", derivation: " + currDerivation(cage)) # comment is derivation string
        chordargs.append(path + "/" + filename)
        c = chord(chordargs) # instantiate a chord
        if wantnotelist:
            c.setnotelist(nl) # note, if notelist not set then chords have no note list appended to bottom (default)
        if scaleopt != 100:
            c.setscalepercent(scaleopt) # default scale is 100%
        c.draw() # draw the chord and save it to given filename
        if web:
            webpage.write( "&lt;td&gt;&lt;img src='" + dr + "/" + urllib.quote_plus(filename) + "'/&gt;&lt;/td&gt;")
        icount += 1
        if icount == rowmax:
            if web:
                webpage.write( "&lt;/tr&gt;&lt;tr&gt;") # start new table row.
            icount = 0

        # finish off writing the chord XML        
        if xml:
            handle.write("&lt;/fretpos&gt;\r\n")     
            handle.write("&lt;derivation&gt;" + currDerivation(cage) + "&lt;/derivation&gt;\r\n") # derivation is from above dictionary
            handle.write("&lt;comment&gt;&lt;/comment&gt;\r\n")
            handle.write("&lt;/chord&gt;\r\n")
            handle.write("\r\n")
        # and write to database
        if sql:
            svgstr = ""
            dbchord = open(path + "/" + filename, 'r') # re-open svg chord just created
            svgstr += dbchord.read()
            dbchord.close()
            createChordtableEntry(conn, key, 'Normal', chordname, chordname, comment, 'genchord.py', 'V1.0', dbfrets, svgstr, currDerivation(cage))
 
    return icount # update caller with amended item count

# creates the Keys Table entries
def createKeyTableEntries (conn):
    cursor = conn.cursor ()
    indx = 0
    for key in keys:
        sql = "INSERT INTO `key` (keyname, alias,comment) VALUES(\'" + key + "\',\'" + aliaskeys[indx] + "\',\'created using genchord.py V1.00!\')\n"
        cursor.execute(sql)
        indx += 1
    conn.commit()

# create a Tuning Table entry
def createTuningTableEntry(cursor, tuningname, tuning):
    # check this tuning dosn't already exist
    sqls = "SELECT idtuning FROM `tuning` WHERE tuningname LIKE \'" + tuningname + "\'\n"
    cursor.execute(sqls)
    row1 = cursor.fetchone()
    if row1 == None: # no, it doesn't exist, so insert it
        sqls = """INSERT INTO `tuning` (tuningname, tuning) VALUES(\'""" + tuningname + """\',\'""" + tuning + """\')\n"""
        cursor.execute(sqls)
    else:
        print("Tuning: " + tuningname + " already exists." # flag user, tuning name already in database)
    
                              

