# and also easier to extend/change
tunings = { "E" : {  "D" : 2, "Eb" : 1, "D#" : 1, "E" : 0, "F" : -1, "F#" : -2, "Gb" : -2},
            "A" : {  "G" : 2, "G#" : 1, "Ab" : 1, "A" : 0, "Bb": -1, "A#" : -1, "B"  : -2},
