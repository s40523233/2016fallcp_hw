            if xml:
                handle.write("&lt;chord instrument='guitar' strings='6' tuning='" + currTuning() + "' position='" + str(inner[0]) + "'&gt;\r\n") # write chord metadata
            key = chordname[0] # prepare to extract the tonal key from the chordname
            if len(chordname) &gt; 1:
                if chordname[1] == '#' or chordname[1] == 'b':
                    length = 2 # prepare to augment tonal key string with sharp or flat
                    key += chordname[1]

            if xml:
                handle.write("&lt;keynote&gt;" + key + "&lt;/keynote&gt;\r\n")
            chordtype = chordname[length:len(chordname)] # obtain the chord type as a substring with tonal key removed
            filename = filenamestart + chordtype
            if xml:
                handle.write("&lt;name&gt;" + chordtype + "&lt;/name&gt;\r\n")
                # now output the fret positions
                handle.write("&lt;fretpos&gt;")
            chordargs = [] # prepare to construct args for SVG generator
            dbfrets = "" # mt string to receive frets to output to database
            for fretpos in range(1,7):
                # ignore fingering, use only 1st item in each sublist
                if  inner[fretpos][0] == -1:
                    if xml:
                        handle.write("X") # write X to XML file
                    chordargs.append ("X") # prepare fret positions for SVG generator
                    dbfrets += "X"
                    nl.append(" ")
                else:
                    calcfret = inner[fretpos][0] + semitones[fretpos-1]
                    if xml:
                        handle.write(str(calcfret))
                    dbfrets += str(calcfret)
                    chordargs.append(calcfret) # prepare fret positions for SVG generator
                    nl.append(getNote(currentTuning[fretpos-1],calcfret))
                if fretpos &lt; 6: # only put spaces betwixt fret posns.
                    if xml:
                        handle.write(' ')
                    dbfrets += ' '

            if xml:
                # finish off writing the chord XML        
                handle.write("&lt;/fretpos&gt;\r\n")     
                handle.write("&lt;derivation&gt;Genesis&lt;/derivation&gt;\r\n") # derivation is from above dictionary
                handle.write("&lt;comment&gt;&lt;/comment&gt;\r\n")
                handle.write("&lt;/chord&gt;\r\n")
                handle.write("\r\n")
            # now prepare arguments for SVG chord generator
            chordargs.append (0) # no capo
            chordargs.append (0) # no cut
            chordargs.append (handed) # right-handed chord
            chordargs.append(chordname)
            chordargs.append(comment)
            chordargs.append(path + "/" + filename + ".svg")
            if web:
                webpage.write( "&lt;td&gt;&lt;img src='" + dr + "/" + urllib.quote_plus(filename) + ".svg'/&gt;&lt;/td&gt;") # note, substitute unacceptable chars for web use
            icount += 1
            if icount == rowmax:
                if web:
                    webpage.write( "&lt;/tr&gt;&lt;tr&gt;") # start new table row.
                icount = 0
            c = chord(chordargs) # instantiate a chord
            if wantnotelist:
                c.setnotelist(nl) # note, if notelist not set then chords have no note list appended to bottom (default)
            if scaleopt != 100:
                c.setscalepercent(scaleopt) # default scale is 100%
            c.draw() # draw the chord and save it to given filename
            # now do the CAGED variations for this chord
            icount = doCagedChord(conn, handle, path, webpage, key, dr, chordtype, comment, icount, xml, web, sql, wantnotelist, scaleopt)
            if sql:
                svgstr = ""
                dbchord = open(path + "/" + filename + ".svg", 'r') # re-open svg chord just created
                svgstr += dbchord.read()
                dbchord.close()
                
                createChordtableEntry(conn, key, 'Normal', chordtype, chordtype, comment, 'genchord.py', 'V1.0', dbfrets, svgstr, 'genesis')

        if xml:
            handle.write("\r\n") # add an empty line to each key chord block in xml file
        if web:
            webpage.write( "&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;") # terminate table for this key
            if webmulti:
                webpage.write("&lt;/BODY&gt;&lt;/HTML&gt;")
                webpage.close() # and close output to key-based webpage file
                
        aliasindx += 1


    if sql:
        conn.close()
    if xml:
        handle.write("&lt;/chords&gt;\r\n") # close the 'top-level' object   
        handle.close() # and the file.
    if web and not webmulti: # close the single large web page.
        webpage.write("&lt;/BODY&gt;&lt;/HTML&gt;")
        webpage.close() # and close output to webpage file

    print("done!")

if __name__ == "__main__":
    sys.exit(main())</t>
